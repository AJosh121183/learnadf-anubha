{
	"name": "Copy_Lego_HTTP_ADLS_Themes",
	"properties": {
		"description": "Copy Themes.csv.gz from rebrickable to ADLS",
		"activities": [
			{
				"name": "Copy_354",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "@{pipeline().parameters.FileName}.csv"
					},
					{
						"name": "Destination",
						"value": "imdb//@{pipeline().parameters.FileName.csv}"
					}
				],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings",
							"skipLineCount": 0
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"logStorageSettings": {
						"linkedServiceName": {
							"referenceName": "ADLS_anubhaadls",
							"type": "LinkedServiceReference"
						},
						"path": "imdb/errors/@{pipeline().parameters.FileName}.csv"
					}
				},
				"inputs": [
					{
						"referenceName": "HTTP_imdb_title",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "@pipeline().parameters.FileName",
								"type": "Expression"
							}
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_imdb_title",
						"type": "DatasetReference",
						"parameters": {
							"FileName": {
								"value": "@pipeline().parameters.FileName",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"FileName": {
				"type": "String",
				"defaultValue": "FileName"
			}
		},
		"annotations": [],
		"lastPublishTime": "2020-09-16T14:57:27Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}